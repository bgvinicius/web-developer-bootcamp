<h1>The posts page!</h1>

<ul>
    <% for (var i = 0; i < posts.length; i++) { %>
        <li><%=posts[i].title;%> - <strong><%=posts[i].text;%></strong></li>
   <% } %>
</ul>

<ul>
    <% posts.forEach(function(post) { %>
        <li><%=post.title;%> - <strong><%=post.text;%></strong></li>
   <% }); %>
</ul>